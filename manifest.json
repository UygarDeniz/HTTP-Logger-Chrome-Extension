{
  "name": "My Extension",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
    
  },
  "action": {
   
    "default_popup": "index.html"
    
  },
  "permissions": ["webRequest", "tabs", "storage", "downloads"],
  "host_permissions": ["<all_urls>"]
}